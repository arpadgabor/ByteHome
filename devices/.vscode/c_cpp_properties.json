{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceRoot}/build/**",
                "${workspaceRoot}/components/**",
                "${workspaceRoot}/**",
                "${env:MDF_PATH}/**",
                "${env:MDF_PATH}/components/**",
                "${env:MDF_PATH}/esp-idf/**",
                "${env:MDF_PATH}/esp-idf/components/**"
            ],
            "intelliSenseMode": "gcc-x64",
            "compilerPath": "${env:HOME}/esp/xtensa-esp32-elf/bin",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "configurationProvider": "vector-of-bool.cmake-tools"
        }
    ],
    "version": 4
}